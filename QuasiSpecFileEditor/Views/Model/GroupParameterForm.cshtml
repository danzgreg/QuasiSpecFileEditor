@model QuasiSpecFileEditor.ViewModels.GroupParameterDisplay

<table class="table table-hover table-sm table-bordered table-condensed">
    <thead class="table-active">
        <tr>
            <th>FielName</th>
            <th>UL</th>
            <th>UL Defect</th>
            <th>LL</th>
            <th>LL Defect</th>
            <th>NF</th>
            <th>NF Defect</th>
            <th>OPReqUpload</th>
            <th>PT</th>
            <th>CSVHeaderName</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var parameter in Model.Parameters)
        {
            <tr>

                <td>@parameter.FieldName</td>
                <td>@parameter.UpperLimit</td>
                <td>@parameter.UpperLimitDefect</td>
                <td>@parameter.LowerLimit</td>
                <td>@parameter.LowerLimitDefect</td>
                <td>@parameter.NegFactor</td>
                <td>@parameter.NegFactorDefect</td>
                <td>@parameter.OpReqUpload</td>
                <td>@parameter.ParamTest</td>
                <td>@parameter.CSVHeaderName</td>

            </tr>
        }
    </tbody>
</table>